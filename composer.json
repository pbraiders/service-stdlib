{
    "name": "pbraiders/stdlib",
    "description": "Pbraiders stdlib component",
    "homepage": "https://github.com/pbraiders/stdlib",
    "readme": "README.md",
    "time": "2019-09-10",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Olivier Jullien",
            "homepage": "https://github.com/ojullien",
            "email": "contact@pbraiders.fr"
        }
    ],
    "require": {
        "php": "^7.2",
        "league/uri-parser": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Pbraiders\\Stdlib\\": "src/"
        },
        "files": [
            "src/Stdlib.php"
        ]
    },
    "require-dev": {
        "php-coveralls/php-coveralls": "^2.1",
        "phpunit/phpunit": "^8"
    },
    "autoload-dev": {
        "psr-4": {
            "PbraidersTest\\Stdlib\\": "tests/"
        },
        "files": [
            "src/Stdlib.php"
        ]
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "vendor-dir": "vendor"
    },
    "scripts": {
        "test": "vendor/bin/phpunit --configuration ./phpunit.xml.dist --colors=always --coverage-clover build/logs/clover.xml"
    }
}
